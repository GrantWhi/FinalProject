<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <TextView
            android:text="List Title"
            android:textSize="30sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ListTitle"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.078"
            app:layout_constraintHorizontal_bias="0.52"/>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="232dp"
            android:layout_height="183dp"
            app:layout_constraintTop_toBottomOf="@+id/ListTitle" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.095" app:layout_constraintVertical_bias="0.109"
            android:id="@+id/linearLayout2">
        <EditText
                android:text="Task 1"
                android:layout_width="match_parent"
                android:textSize="10dp"
                android:layout_height="wrap_content" android:id="@+id/TaskOne"/>
        <EditText
                android:text="Task 2"
                android:layout_width="match_parent"
                android:textSize="10dp"
                android:layout_height="wrap_content" android:id="@+id/TaskTwo"/>
        <EditText
                android:text="Task 3"
                android:layout_width="match_parent"
                android:textSize="10dp"
                android:layout_height="wrap_content" android:id="@+id/TaskThree"/>
        <EditText
                android:text="Task 4"
                android:layout_width="match_parent"
                android:textSize="10dp"
                android:layout_height="wrap_content" android:id="@+id/TaskFour"/>
        <EditText
                android:text="Task 5"
                android:layout_width="match_parent"
                android:textSize="10dp"
                android:layout_height="wrap_content" android:id="@+id/TaskFive"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="130dp"
            android:layout_height="183dp"
            app:layout_constraintTop_toTopOf="@+id/linearLayout2"
            app:layout_constraintBottom_toBottomOf="@+id/linearLayout2"
            app:layout_constraintStart_toEndOf="@+id/linearLayout2" app:layout_constraintEnd_toEndOf="parent">
        <TextView
                android:text="%"
                android:layout_width="match_parent"
                android:layout_height="34dp" android:id="@+id/WeightOne"/>
        <TextView
                android:text="%"
                android:layout_width="match_parent"
                android:layout_height="34dp" android:id="@+id/WeightTwo"/>
        <TextView
                android:text="%"
                android:layout_width="match_parent"
                android:layout_height="34dp" android:id="@+id/WeightThree"/>
        <TextView
                android:text="%"
                android:layout_width="match_parent"
                android:layout_height="34dp" android:id="@+id/WeightFour"/>
        <TextView
                android:text="%"
                android:layout_width="match_parent"
                android:layout_height="34dp" android:id="@+id/WeightFive"/>
    </LinearLayout>
    <TextView
            android:text="%"
            android:layout_width="244dp"
            android:textSize="30sp"
            android:layout_height="73dp" android:id="@+id/OverallCompletion"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintVertical_bias="0.595"/>
    <Button
            android:text="Home"
            android:layout_width="128dp"
            android:layout_height="84dp" android:id="@+id/Activity1"
            android:onClick="GoToMainActivity"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.176"
            app:layout_constraintVertical_bias="0.744"/>
    <Button
            android:text="Settings"
            android:layout_width="128dp"
            android:layout_height="84dp" android:id="@+id/Preferences"
            android:onClick="GoToPreferencesActivity"
            app:layout_constraintTop_toTopOf="@+id/Activity1"
            app:layout_constraintBottom_toBottomOf="@+id/Activity1" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.826"
            app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="Help"
            android:layout_width="128dp"
            android:layout_height="84dp" android:id="@+id/Help"
            android:onClick="GoToHelpActivity"
            app:layout_constraintTop_toBottomOf="@+id/Activity1"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>